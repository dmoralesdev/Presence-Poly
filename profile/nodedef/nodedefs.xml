<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller -->
    <nodeDef id="presence_controller" nls="ctl">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
        </sts>
        <cmds>
            <sends />
            <accepts>
              <cmd id="DISCOVER" />
              <cmd id="REMOVE_NOTICES_ALL" />
              <cmd id="UPDATE_PROFILE" />
            </accepts>
        </cmds>
    </nodeDef>
	<!-- mynodetype -->
    <nodeDef id="presence_node" nls="pnode">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
            <st id="GV0" editor="proximity" />
            <st id="GV1" editor="bool" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
